{
  "id": null,
  "uid": "gasdf-monitoring",
  "title": "GASdf Monitoring",
  "description": "GASdf - Gasless transactions for Solana",
  "tags": ["gasdf", "solana"],
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 0,
  "panels": [
    {
      "id": 1,
      "type": "text",
      "title": "",
      "gridPos": { "h": 2, "w": 24, "x": 0, "y": 0 },
      "options": {
        "mode": "markdown",
        "content": "# GASdf Monitoring\nGasless transactions for Solana - Pay fees with any token, burn $ASDF"
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Total Quotes",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 2 },
      "targets": [
        {
          "expr": "gasdf_quotes_total{service=\"gasdf\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "blue", "value": null }]
          }
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Total Submits",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 2 },
      "targets": [
        {
          "expr": "gasdf_submits_total{service=\"gasdf\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          }
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Fee Payer SOL",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 2 },
      "targets": [
        {
          "expr": "gasdf_fee_payer_balance_lamports{service=\"gasdf\"} / 1000000000",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 4,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "green", "value": 0.5 }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Total Burned",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 2 },
      "targets": [
        {
          "expr": "gasdf_asdf_burned_total{service=\"gasdf\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "orange", "value": null }]
          }
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Request Rate",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "targets": [
        {
          "expr": "rate(gasdf_http_requests_total{service=\"gasdf\"}[5m])",
          "legendFormat": "{{route}} - {{status}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Response Time",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "targets": [
        {
          "expr": "gasdf_http_request_duration_seconds{service=\"gasdf\",quantile=\"0.95\"}",
          "legendFormat": "P95 {{route}}",
          "refId": "A"
        },
        {
          "expr": "gasdf_http_request_duration_seconds{service=\"gasdf\",quantile=\"0.5\"}",
          "legendFormat": "P50 {{route}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Quotes & Submits",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "targets": [
        {
          "expr": "gasdf_quotes_total{service=\"gasdf\"}",
          "legendFormat": "Quotes",
          "refId": "A"
        },
        {
          "expr": "gasdf_submits_total{service=\"gasdf\"}",
          "legendFormat": "Submits",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          }
        }
      }
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "Fee Payer Balance History",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
      "targets": [
        {
          "expr": "gasdf_fee_payer_balance_lamports{service=\"gasdf\"} / 1000000000",
          "legendFormat": "SOL Balance",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 4,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 30
          },
          "color": { "mode": "fixed", "fixedColor": "yellow" }
        }
      }
    }
  ],
  "time": { "from": "now-1h", "to": "now" },
  "refresh": "30s",
  "schemaVersion": 17,
  "version": 0,
  "templating": { "list": [] },
  "annotations": { "list": [] },
  "links": []
}
