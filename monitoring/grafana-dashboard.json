{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "Prometheus datasource for GASdf metrics",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "9.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": ""
    },
    {
      "type": "panel",
      "id": "text",
      "name": "Text",
      "version": ""
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": ""
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "#E040FB",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "GASdf - Gasless transactions for Solana. Pay fees with any token, burn $ASDF. Built with Helius RPC.",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [
    {
      "asDropdown": false,
      "icon": "external link",
      "includeVars": false,
      "keepTime": false,
      "tags": [],
      "targetBlank": true,
      "title": "GASdf API",
      "tooltip": "",
      "type": "link",
      "url": "https://gasdf-43r8.onrender.com"
    },
    {
      "asDropdown": false,
      "icon": "external link",
      "includeVars": false,
      "keepTime": false,
      "tags": [],
      "targetBlank": true,
      "title": "Helius",
      "tooltip": "",
      "type": "link",
      "url": "https://helius.dev"
    },
    {
      "asDropdown": false,
      "icon": "doc",
      "includeVars": false,
      "keepTime": false,
      "tags": [],
      "targetBlank": true,
      "title": "GitHub",
      "tooltip": "",
      "type": "link",
      "url": "https://github.com/zeyxx/GASdf"
    }
  ],
  "liveNow": false,
  "panels": [
    {
      "gridPos": { "h": 2, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "options": {
        "code": { "language": "plaintext", "showLineNumbers": false, "showMiniMap": false },
        "content": "<div style=\"text-align: center;\">\n<span style=\"font-size: 24px; font-weight: bold; background: linear-gradient(90deg, #E040FB, #7C4DFF); -webkit-background-clip: text; -webkit-text-fill-color: transparent;\">GASdf Monitoring</span>\n<br/>\n<span style=\"color: #B388FF; font-size: 14px;\">Gasless Transactions for Solana â€¢ Powered by Helius RPC</span>\n</div>",
        "mode": "html"
      },
      "title": "",
      "transparent": true,
      "type": "text"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Total quote requests processed",
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "#7C4DFF", "mode": "fixed" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "#7C4DFF", "value": null }] },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 2 },
      "id": 2,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "gasdf_quotes_total{service=\"gasdf\"}",
          "legendFormat": "Quotes",
          "refId": "A"
        }
      ],
      "title": "Total Quotes",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Transactions successfully submitted",
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "#00E676", "mode": "fixed" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "#00E676", "value": null }] },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 2 },
      "id": 3,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "gasdf_submits_total{service=\"gasdf\"}",
          "legendFormat": "Submits",
          "refId": "A"
        }
      ],
      "title": "Total Submits",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Treasury wallet SOL balance for gas fees",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "decimals": 4,
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "#FF5252", "value": null },
              { "color": "#FFD740", "value": 0.1 },
              { "color": "#00E676", "value": 0.5 }
            ]
          },
          "unit": "none"
        },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 6, "x": 12, "y": 2 },
      "id": 4,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "gasdf_fee_payer_balance_lamports{service=\"gasdf\"} / 1000000000",
          "legendFormat": "SOL",
          "refId": "A"
        }
      ],
      "title": "Treasury SOL",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Total $ASDF tokens burned forever",
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "#E040FB", "mode": "fixed" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "#E040FB", "value": null }] },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 5, "w": 6, "x": 18, "y": 2 },
      "id": 5,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "gasdf_asdf_burned_total{service=\"gasdf\"}",
          "legendFormat": "Burned",
          "refId": "A"
        }
      ],
      "title": "$ASDF Burned ðŸ”¥",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "API requests per second via Helius RPC",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "#7C4DFF", "value": null }] },
          "unit": "reqps"
        },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": ".*200.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "#00E676", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byRegexp", "options": ".*4[0-9]{2}.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "#FFD740", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byRegexp", "options": ".*5[0-9]{2}.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "#FF5252", "mode": "fixed" } }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 7 },
      "id": 6,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "editorMode": "code",
          "expr": "rate(gasdf_http_requests_total{service=\"gasdf\"}[5m])",
          "legendFormat": "{{route}} [{{status}}]",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Request Rate",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Helius RPC latency - P50 (typical) and P95 (worst case)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 15,
            "gradientMode": "opacity",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "#7C4DFF", "value": null }] },
          "unit": "ms"
        },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": "P95.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "#E040FB", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byRegexp", "options": "P50.*" },
            "properties": [{ "id": "color", "value": { "fixedColor": "#B388FF", "mode": "fixed" } }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 7 },
      "id": 7,
      "options": {
        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "editorMode": "code",
          "expr": "gasdf_http_request_duration_seconds{service=\"gasdf\",quantile=\"0.95\"} * 1000",
          "legendFormat": "P95 {{route}}",
          "range": true,
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "editorMode": "code",
          "expr": "gasdf_http_request_duration_seconds{service=\"gasdf\",quantile=\"0.5\"} * 1000",
          "legendFormat": "P50 {{route}}",
          "range": true,
          "refId": "B"
        }
      ],
      "title": "Response Latency",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Conversion funnel: Quotes requested vs Transactions submitted",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 30,
            "gradientMode": "opacity",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": { "mode": "off" }
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "#7C4DFF", "value": null }] },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Quotes" },
            "properties": [{ "id": "color", "value": { "fixedColor": "#7C4DFF", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Submits" },
            "properties": [{ "id": "color", "value": { "fixedColor": "#00E676", "mode": "fixed" } }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "id": 8,
      "options": {
        "legend": { "calcs": ["last", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "editorMode": "code",
          "expr": "gasdf_quotes_total{service=\"gasdf\"}",
          "legendFormat": "Quotes",
          "range": true,
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "editorMode": "code",
          "expr": "gasdf_submits_total{service=\"gasdf\"}",
          "legendFormat": "Submits",
          "range": true,
          "refId": "B"
        }
      ],
      "title": "Quote â†’ Submit Funnel",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Treasury SOL balance - Alert threshold at 0.1 SOL",
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "#FFD740", "mode": "fixed" },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "SOL",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 35,
            "gradientMode": "opacity",
            "hideFrom": { "legend": false, "tooltip": false, "viz": false },
            "insertNulls": false,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": { "type": "linear" },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "group": "A", "mode": "none" },
            "thresholdsStyle": {
              "mode": "line+area"
            }
          },
          "decimals": 4,
          "mappings": [],
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "#FF5252", "value": null },
              { "color": "transparent", "value": 0.1 }
            ]
          },
          "unit": "none"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "id": 9,
      "options": {
        "legend": { "calcs": ["last", "min"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "editorMode": "code",
          "expr": "gasdf_fee_payer_balance_lamports{service=\"gasdf\"} / 1000000000",
          "legendFormat": "Treasury Balance",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Treasury Balance",
      "type": "timeseries"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": ["gasdf", "solana", "helius", "asdf"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d"]
  },
  "timezone": "browser",
  "title": "GASdf Monitoring",
  "uid": "gasdf-monitoring",
  "version": 1,
  "weekStart": ""
}
