# GASdf Metrics Agent Configuration
# Copy to .env and fill in values from Grafana Cloud

# GASdf API metrics key (from Render env vars)
METRICS_API_KEY=your_metrics_api_key

# Grafana Cloud credentials (from grafana.com → My Account → Prometheus)
GRAFANA_REMOTE_WRITE_URL=https://prometheus-prod-XX-prod-XX.grafana.net/api/prom/push
GRAFANA_USER_ID=123456
GRAFANA_API_KEY=glc_xxxxxxxxxxxxx
