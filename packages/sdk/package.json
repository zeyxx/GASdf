{
  "name": "gasdf-sdk",
  "version": "1.0.2",
  "description": "GASdf SDK - Add gasless transactions to your Solana app",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "solana",
    "gasless",
    "meta-transactions",
    "sdk",
    "react"
  ],
  "author": "GASdf",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zeyxx/GASdf.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/zeyxx/GASdf#readme",
  "bugs": {
    "url": "https://github.com/zeyxx/GASdf/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.87.0",
    "@solana/wallet-adapter-react": "^0.15.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "@solana/wallet-adapter-react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/web3.js": "^1.95.8",
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.7",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
